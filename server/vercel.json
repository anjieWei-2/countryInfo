{
    "version": 2,
    "builds": [
      {
        "src": "server.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],
    "engines": {
        "node": "18.x"
      },
    "rewrites": [
        { "source": "/api/(.*)", "destination": "/api" }]
      
}